{
  "version": "2.40.2",
  "license": "MIT",
  "name": "@solana-foundation/solana-lib",
  "main": "./src/index.ts",
  "typings": "./src/index.ts",
  "sideEffects": [
    "**/*.scss"
  ],
  "engines": {
    "node": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "staging",
        "prerelease": "staging"
      }
    ],
    "plugins": [
      "@semantic-release/npm",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/commit-analyzer",
      "@semantic-release/git",
      "@semantic-release/changelog"
    ]
  },
  "scripts": {
    "start": "tsdx watch",
    "prebuild": "rimraf dist",
    "build": "tsdx build && yarn build-tailwind",
    "lint": "tsdx lint",
    "size": "size-limit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "serve-storybook": "serve storybook-static",
    "build-tailwind": "NODE_ENV=production npx tailwindcss -i ./src/tailwind.css -o ./dist/styles.css --minify",
    "check-types": "tsc --noEmit",
    "gen:icons": "node tooling/gen-sprite.mjs"
  },
  "peerDependencies": {
    "next": ">=13",
    "react": ">=16"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^10.0.3",
    "@semantic-release/release-notes-generator": "^11.0.1",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@storybook/addon-essentials": "^7.1.0-alpha.30",
    "@storybook/addon-info": "^6.0.0-alpha.2",
    "@storybook/addon-interactions": "^7.1.0-alpha.30",
    "@storybook/addon-links": "^7.1.0-alpha.30",
    "@storybook/addon-styling": "^1.0.8",
    "@storybook/addons": "^7.1.0-alpha.30",
    "@storybook/blocks": "^7.1.0-alpha.30",
    "@storybook/cli": "^7.1.0-alpha.30",
    "@storybook/nextjs": "^7.0.20",
    "@storybook/react": "^7.1.0-alpha.30",
    "@storybook/react-webpack5": "^7.1.0-alpha.30",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/dompurify": "^3.0.2",
    "@types/prismjs": "^1.26.3",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "autoprefixer": "^10.4.20",
    "cssnano": "^6.0.1",
    "husky": "^9.1.6",
    "next": "^15.1.0",
    "postcss": "^8.4.47",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-is": "^19.0.0",
    "rimraf": "^5.0.1",
    "storybook": "^7.1.0-alpha.30",
    "svg-spreact": "^1.3.2",
    "tailwindcss": "^3.4.14",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.3",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@radix-ui/react-tooltip": "^1.0.6",
    "@solana-foundation/typescript-config": "workspace:^",
    "@types/react-slick": "^0.23.13",
    "class-variance-authority": "^0.7.0",
    "html-react-parser": "^5.1.10",
    "prismjs": "^1.29.0",
    "react-countup": "^6.5.3",
    "react-fast-marquee": "^1.6.0",
    "react-feather": "^2.0.10",
    "react-player": "^2.16.0",
    "react-share": "^5.1.0",
    "react-slick": "^0.30.2",
    "sass": "^1.80.3",
    "slick-carousel": "^1.8.1",
    "slugify": "^1.6.6",
    "tailwind-merge": "^1.14.0"
  }
}
